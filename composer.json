{
  "name": "treehouselabs/cache-bundle",
  "type": "symfony-bundle",
  "description": "Symfony2 bundle that provides easy setup for caching, including a powerful ORM cache",
  "keywords": ["cache"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Kruithof",
      "email": "peter@treehouse.nl"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.4",
    "symfony/symfony": "~2.3",
    "doctrine/orm": "~2.2,>=2.2.3",
    "doctrine/doctrine-bundle": "~1.2",
    "treehouselabs/cache": "dev-master as 1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "psr-4": { "TreeHouse\\CacheBundle\\": "src/TreeHouse/CacheBundle/" }
  }
}
